

    <h1>Cambiar Rol de Usuario</h1>
    <form id="changeRoleForm">
        <label for="userId">ID de Usuario:</label>
        <input type="text" id="userId" name="userId" required><br><br>
        
        <label for="role">Nuevo Rol:</label>
        <select id="role" name="role">
            <option value="user">Usuario</option>
            <option value="premium">Premium</option>
            <option value="admin">Administrador</option>
        </select><br><br>

        <button id="btn" type="submit">Cambiar Rol</button>
        <button id="btn" type="submit"> Eliminar Usuario</button>
    </form>

    <a href="/api/session/allUsers">Ver todos los usuarios disponibles</a>

    <script src='/js/changeRoleAdmi.js'></script>
    